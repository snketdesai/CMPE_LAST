		<hr>
		<div class="col-md-12 text-center">
			<p>CmpE-282 CloudServices-LinkedIn Prototype</p>
		</div>
		<hr>

	</div>
	</div>
	<!--/main-->

	<!-- script references -->
	<script src="js/jquery-1.11.2.min.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/scripts.js"></script>
